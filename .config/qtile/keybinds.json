{
    "": {
        "Move Focus To Left": {
            "keybind": "[MOD] + h",
            "command": "left "
        },
        "Move Focus To Right": {
            "keybind": "[MOD] + l",
            "command": "right "
        },
        "Move Focus Down": {
            "keybind": "[MOD] + j",
            "command": "down "
        },
        "Move Focus Up": {
            "keybind": "[MOD] + k",
            "command": "up "
        },
        "Change Keyboard Layout": {
            "keybind": "[MOD] + Space",
            "command": "next_keyboard "
        },
        "Move Window To The Left": {
            "keybind": "[MOD] + [CTRL] + h",
            "command": "shuffle_left "
        },
        "Move Window To The Right": {
            "keybind": "[MOD] + [CTRL] + l",
            "command": "shuffle_right "
        },
        "Move Window Down": {
            "keybind": "[MOD] + [CTRL] + j",
            "command": "shuffle_down "
        },
        "Move Window Up": {
            "keybind": "[MOD] + [CTRL] + k",
            "command": "shuffle_up "
        },
        "Grow Window To The Left": {
            "keybind": "[MOD] + [SHIFT] + h",
            "command": "grow_left "
        },
        "Grow Window To The Right": {
            "keybind": "[MOD] + [SHIFT] + l",
            "command": "grow_right "
        },
        "Grow Window Down": {
            "keybind": "[MOD] + [SHIFT] + j",
            "command": "grow_down "
        },
        "Grow Window Up": {
            "keybind": "[MOD] + [SHIFT] + k",
            "command": "grow_up "
        },
        "Reset All Window Sizes": {
            "keybind": "[MOD] + n",
            "command": "normalize "
        },
        "": {
            "keybind": "[CTRL] + 3",
            "command": "dropdown_toggle 'notifications'"
        },
        "Toggle Between Split And Unsplit Sides Of Stack": {
            "keybind": "[MOD] + [SHIFT] + Return",
            "command": "toggle_split "
        },
        "Launch Terminal": {
            "keybind": "[MOD] + Return",
            "command": "spawn 'alacritty'"
        },
        "Toggle Previous Layout": {
            "keybind": "[MOD] + [SHIFT] + Tab",
            "command": "prev_layout "
        },
        "Toggle Next Layout": {
            "keybind": "[MOD] + Tab",
            "command": "next_layout "
        },
        "Kill Focused Window": {
            "keybind": "[MOD] + q",
            "command": "kill "
        },
        "Reload The Config": {
            "keybind": "[MOD] + [CTRL] + r",
            "command": "reload_config "
        },
        "Shutdown Qtile": {
            "keybind": "[MOD] + [CTRL] + q",
            "command": "shutdown "
        },
        "Spawn A Command Using A Prompt Widget": {
            "keybind": "[MOD] + r",
            "command": "spawn 'rofi -show drun'"
        },
        "Show All Currently Open Windows": {
            "keybind": "[MOD] + w",
            "command": "spawn 'rofi -show window'"
        },
        "Powermenu": {
            "keybind": "[MOD] + p",
            "command": "spawn 'sh -c ~/.config/rofi/scripts/power'"
        },
        "Theme_Switcher": {
            "keybind": "[MOD] + t",
            "command": "spawn 'sh -c ~/.config/rofi/scripts/themes'"
        },
        "File Manager": {
            "keybind": "[MOD] + e",
            "command": "spawn 'thunar'"
        },
        "Clipboard": {
            "keybind": "[MOD] + h",
            "command": "spawn 'roficlip'"
        },
        "Screenshot": {
            "keybind": "[MOD] + s",
            "command": "spawn 'flameshot gui'"
        },
        "Launch Notes": {
            "keybind": "[CTRL] + 1",
            "command": "dropdown_toggle 'notes'"
        },
